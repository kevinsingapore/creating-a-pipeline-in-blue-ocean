#!/usr/bin/env groovy
pipeline {
  agent any

  stages {
    stage('jenkins_kevin') {
        steps {
            echo 'jenkins_kevin..'
        }
    }
    stage('Build') {
        steps {
            echo 'Building..'
        }
    }
    stage('Test') {
        steps {
            echo 'Testing..'
        }
    }
    stage('Deploy') {
        steps {
            echo 'Deploying....'
        }
    }
  }
}
